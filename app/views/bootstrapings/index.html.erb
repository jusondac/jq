<p id="notice"><%= notice %></p>

<h1>Bootstrapings</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Code</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bootstrapings.each do |bootstraping| %>
      <tr>
        <td><%= bootstraping.title %></td>
        <td><%= bootstraping.code %></td>
        <td><%= link_to 'Show', bootstraping %></td>
        <td><%= link_to 'Edit', edit_bootstraping_path(bootstraping) %></td>
        <td><%= link_to 'Destroy', bootstraping, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bootstraping', new_bootstraping_path %>
